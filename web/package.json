    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "focus-trap": "^7.5.4",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "hex-to-rgba": "^2.0.1",
    "lucide-vue-next": "^0.445.0",
    "marked": "^12.0.2",
    "marked-base-url": "^1.1.3",
    "radix-vue": "^1.9.6",
    "semver": "^7.6.2",
    "tailwind-merge": "^2.5.2",
