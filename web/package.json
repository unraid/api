    "vite": "7.1.3",
    "vite-plugin-remove-console": "2.2.0",
    "vite-plugin-vue-tracer": "1.0.0",
    "vitest": "3.2.4",
    "vue": "3.5.20",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.6"
  },
  "dependencies": {
    "@apollo/client": "3.14.0",
