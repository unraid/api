{
    "name": "@unraid/api",
    "version": "4.0.1",
    "main": "src/cli/index.ts",
    "type": "module",
    "corepack": {
        "enabled": true
    },
    "repository": "git@github.com:unraid/api.git",
    "author": "Lime Technology, Inc. <unraid.net>",
    "license": "UNLICENSED",
    "engines": {
        "pnpm": ">=8.15.4"
