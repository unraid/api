    "graphql": "^16.8.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "graphql-type-uuid": "^0.2.0",
    "graphql-ws": "^5.14.3",
    "htpasswd-js": "^1.0.2",
    "ini": "^4.1.1",
    "ip": "^1.1.8",
    "jose": "^4.14.2",
    "lodash": "^4.17.21",
    "multi-ini": "^2.2.0",
    "mustache": "^4.2.0",
    "nanobus": "^4.5.0",
    "nest-access-control": "^3.1.0",
    "nestjs-pino": "^4.0.0",
    "node-cache": "^5.1.2",
    "node-window-polyfill": "^1.0.2",
    "openid-client": "^5.6.4",
    "p-iteration": "^1.1.8",
    "p-retry": "^4.6.2",
    "passport-http-header-strategy": "^1.1.0",
    "pidusage": "^3.0.2",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.1.14",
    "request": "^2.88.2",
    "semver": "^7.5.4",
    "stoppable": "^1.1.0",
    "systeminformation": "^5.21.22",
    "ts-command-line-args": "^2.5.1",
    "uuid": "^9.0.1",
    "ws": "^8.13.0",
    "wtfnode": "^0.9.1",
    "xhr2": "^0.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
